                    "placeholder": "Select Site(s)",
                    filter:true
                });
            }
        });
         /*GetWTG(false);*/
    }

    function exportToExcel() {

        result = exceldata;
        var columns = [
            "Date", "Site", "WTG", "From Time", "To Time", "Avg Active Power", "Avg Wind Speed", "Restructive WTG", "Expected Power"
            , "Deviation kW", "Loss kWh", "Manual Breakdown ", "All Breakdown"
        ];

        var rows = [];
        for (var i = 0; i < result.length; i++) {
            var row = [];
            var status = "";
            if (result[i].status_code === 1) {
                status = "Missing";
            }
            if (result[i].status_code === 0) {
                status = "Available";
            }
            var formattedDate = moment(result[i].date, 'YYYY/MM/DD').format('DD-MM-YYYY');
            row.push(result[i].site);
            row.push(formattedDate);
            row.push( result[i].wtGs);
            row.push(toHoursAndMinutes(result[i].from_time.totalSeconds));
            row.push(toHoursAndMinutes(result[i].to_time.totalSeconds));
            row.push(result[i].avg_active_power);
            row.push(result[i].avg_wind_speed);
            row.push(result[i].restructive_WTG);
            row.push(result[i].plC_max);
            row.push(result[i].plC_min);
            row.push(result[i].plc_state_code);
            row.push(status);
            row.push(result[i].recon_wind_speed);
            row.push(result[i].exp_power_kw);
            row.push(result[i].deviation_kw);
            row.push(result[i].loss_kw);
            row.push(result[i].manual_bd);
            row.push(result[i].all_bd);

            rows.push(row);
        }

        var data = [columns].concat(rows);

        var ws = XLSX.utils.aoa_to_sheet(data);
        var wb = XLSX.utils.book_new();
        XLSX.utils.book_append_sheet(wb, ws, "Wind Expected Vs Actual");
        var wbout = XLSX.write(wb, { bookType: 'xlsx', type: 'binary' });

        function s2ab(s) {
            var buf = new ArrayBuffer(s.length);
            var view = new Uint8Array(buf);
            for (var i = 0; i !== s.length; ++i) view[i] = s.charCodeAt(i) & 0xFF;
            return buf;
        }

        saveAs(new Blob([s2ab(wbout)], { type: "application/octet-stream" }), "Wind Expected Vs Actual.xlsx");

    }
