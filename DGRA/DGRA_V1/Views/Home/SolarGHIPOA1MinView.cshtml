        yaxisdata.push({ id: 'A', type: 'linear', position: 'left' });
        if ((amp_temp_arr.length > 0 && amp_temp_arr.length != ampTempZeroArr.length) || (mod_arr_arr.length > 0 && mod_arr_arr.length != modZeroArr.length)) {
            dataset_arr.push({ yAxisID: 'B', label: "Amb Temp", data: amp_temp_arr, pointRadius: 0.5, borderColor: "#FFCA5A", fill: false, borderWidth: 1 });
            dataset_arr.push({ yAxisID: 'B', label: "Mod Temp", data: mod_arr_arr, pointRadius: 0.5, borderColor: "#31576D", fill: false, borderWidth: 1 });
            yaxisdata.push({ id: 'B', type: 'linear', position: 'right' });
        }

        if (window.myCharts != undefined) {
            window.myCharts.destroy();
        }

        window.myCharts =  new Chart(document.getElementById("line-chart"), {
            type: 'line',
            data: {
                labels: time_arr_arr,
                datasets: dataset_arr,
            },
            options: {
                title: {
                    display: true,
                    text: 'WMS Compatison'
                },
                responsive: true,
                /*tooltips: {
                    mode: 'index'
                },*/
                hover: {
                    mode: 'nearest',
                    intersect: true
                },
                legend: {
                    display: true,
                    position: "bottom"
                },
                scales: {
                    yAxes: yaxisdata,
                }

            }
        });
        dataset_arr = [];
        time_arr_arr = [];
    }

</script>




